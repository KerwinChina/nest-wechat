{
  "name": "nest-wechat",
  "version": "0.0.10",
  "description": "nestjs module for wechat",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"{lib,test}/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "test": "jest --config ./tests/jest-e2e.json --runInBand",
    "serve:docs": "docsify serve docs",
    "postversion": "git push --follow-tags",
    "release:dry": "release-it --dry-run",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baaxl9vh/nest-wechat.git"
  },
  "keywords": [
    "nest",
    "nestjs",
    "wechat"
  ],
  "author": "jay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baaxl9vh/nest-wechat/issues"
  },
  "homepage": "https://github.com/baaxl9vh/nest-wechat#readme",
  "devDependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/testing": "^8.2.4",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "cross-env": "^7.0.3",
    "docsify-cli": "^4.4.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
